{
  "name": "visor",
  "version": "0.1.0",
  "description": "A lightweight website analytics tool built with React, Node.js, Express, and MongoDB. This tool is a more simple alternative to Google Analytics and other similar products.",
  "homepage": "https://github.com/Cat333t/visor#readme",
  "bugs": {
    "url": "https://github.com/Cat333t/visor/issues"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.5.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-openid-connect": "^2.19.2",
    "helmet": "^8.1.0",
    "ldrs": "^1.1.7",
    "web-vitals": "^2.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cat333t/visor.git"
  },
  "license": "MIT",
  "author": "",
  "type": "commonjs",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "build": "cd client && npm run build && cd ..",
    "server": "nodemon src/index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "deploy": "cd client && npm i && npm run build && cd .."
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "gh-pages": "^6.3.0"
  }
}
